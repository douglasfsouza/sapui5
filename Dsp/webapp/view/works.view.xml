<mvc:View
	controllerName="dgs.dsp.controller.DspList"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<List
		id="invoiceList"
		class="sapUiResponsiveMargin"
		width="auto"
		items="{
			path : '/StatusList',
			sorter : {
				path : 'Description',
				group : true
			}
		}">
		<headerToolbar>
			<Toolbar>
				<Title text="Ttitulo"/>
				<ToolbarSpacer/>
				<SearchField width="50%" search=".onFilterInvoices"/>
			</Toolbar>
		</headerToolbar>
		<items>
			<ObjectListItem
				title="{StatusList/Description}">
				
				 
				 
				 
			</ObjectListItem>
		</items>
	</List>
</mvc:View>
