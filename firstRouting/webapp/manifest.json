{
    "_version": "1.12.0",
    "sap.app": {
      "id": "dgs.routing",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "title": "{{appTitle}}",
      "description": "{{appDescription}}",
      "applicationVersion": {
        "version": "1.0.0"
      }
    },
    "sap.ui": {
      "technology": "UI5",
      "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
      }
    },
    "sap.ui5": {
      "rootView": {
          "viewName": "dgs.routing.view.App",
          "type": "XML",
          "id": "app"
      },
      "dependencies": {
        "minUI5Version": "1.93",
        "libs": {
          "sap.ui.core": {},
          "sap.m": {}
        }
      },
      "models": {
        "i18n": {
          "type": "sap.ui.model.resource.ResourceModel",
          "settings": {
            "bundleName": "sap.ui.demo.walkthrough.i18n.i18n",
            "supportedLocales": [""],
            "fallbackLocale": ""
          }
        }
      },
      "routing": {
        "config":{
          "routerClass": "sap.m.routing.Router",
          "type": "View",
          "viewType": "XML",
          "path": "dgs.routing.view",
          "controlId": "app",
          "controlAggregation": "pages"
        },
        "routes": [
          {
            "pattern": "",
            "name": "mylist",
            "target": "mylist"
          },
          {
            "pattern": "d",
            "name": "mydetail",
            "target": "mydetail"
          } 
        ],
        "targets": {
          "mylist": {
            "id": "MyList",
            "name": "MyList"
          },
          "mydetail": {
            "id": "MyDetail",
            "name": "MyDetail"
          }
        }
      }
      
    }
  }